<%= form_for [@hotel] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :postcode %>
  <%= f.text_field :postcode %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :country %>
  <%= f.text_field :country %>

  <%= f.label :standing %>
  <%= f.text_field :standing %>

  <%= f.submit "Create hotel" %>
<% end %>
