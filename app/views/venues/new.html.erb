<%= form_for [@venue] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :postcode %>
  <%= f.text_field :postcode %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :country %>
  <%= f.text_field :country %>

  <%= f.label :capacity %>
  <%= f.text_field :capacity %>

  <%= f.submit "Create venue" %>
<% end %>
